          <h1 class="ui-kit-nav__branding__title">
            SRE UI Kit
          </h1>
        </div>
        <button id="mobileMenuButton" class="ui-kit-nav__mobile-trigger" type="button">
          <span class="ui-kit-nav__hamburger">
            <span class="ui-kit-nav__hamburger__inner" />
          </span>
        </button>
        <div id="mobileMenuConainer" class="ui-kit-nav__mobile-container">
          <ul class="ui-kit-nav__menu">
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/colors", do: " ui-kit-nav__item--current" %>" href="/ui-kit/colors">
                Colors
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/typography", do: " ui-kit-nav__item--current" %>" href="/ui-kit/typography">
                Typography
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/icons", do: " ui-kit-nav__item--current" %>" href="/ui-kit/icons">
                Icons
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/buttons", do: " ui-kit-nav__item--current" %>" href="/ui-kit/buttons">
                Buttons
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/form-elements", do: " ui-kit-nav__item--current" %>" href="/ui-kit/form-elements">
                Form Elements
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/content-types", do: " ui-kit-nav__item--current" %>" href="/ui-kit/content-types">
                Content Types
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/notification", do: " ui-kit-nav__item--current" %>" href="/ui-kit/notification">
                Notification
              </a>
            </li>
            <li>
              <a class="ui-kit-nav__item<%= if @conn.request_path == "/ui-kit/location-components", do: " ui-kit-nav__item--current" %>" href="/ui-kit/location-components">
                Location Components
              </a>
            </li>
          </ul>
        </div>
      </nav>
      <main class="ui-kit-layout__content" role="main">
        <%= render @view_module, @view_template, assigns %>
      </main>
    </div>
    <script>
      (function() {
        var menuOpen = false;
        var mobileMenuButton = document.getElementById('mobileMenuButton');
        var mobileMenuConainer = document.getElementById('mobileMenuConainer');

        mobileMenuButton.onclick = function(event) {
          if (menuOpen) {
            mobileMenuButton.classList.remove('ui-kit-nav__mobile-trigger--mobile-visible');
            mobileMenuConainer.classList.remove('ui-kit-nav__mobile-container--visible');
          } else {
            mobileMenuButton.classList.add('ui-kit-nav__mobile-trigger--mobile-visible');
            mobileMenuConainer.classList.add('ui-kit-nav__mobile-container--visible');
          }

          menuOpen = !menuOpen;
        }
      })();
    </script>
  </body>
</html>
