      <div style="width: 100%; float: left;">
      <%= Sre.UI.Form.Textarea.render_template id: "message", label: "Message", placeholder: "" %>
      <div class="u-text-right">
        <%= Sre.UI.Button.ButtonDefault.render_template value: "Send", classname: "button", id: "generic-contact"%>
      </div>
      </div>
  </form>
  </article>
</div>

